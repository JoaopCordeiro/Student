
package student;

public class stuu {

	public static void main(String[] args) {
		  
		Person p1 = new Person("Arial", 60);
		Person p2 = new Person("Joseph", 0);
		
		if (p1.getAge()==p2.getAge())
		{
			System.out.println("pergute a ideade");
			System.out.println(p1.getName()+" É a mesa idade que "+p2.getName());
		}
		else
		{
			System.out.println(p1.getName()+" não é a mesma idade que "+p2.getName());
			
		}

	}

}
